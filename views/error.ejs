<% layout('/layouts/boilerplate') %>
    <div class="row">
        <div class="col-6 offset-3">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading">Error</h4>
                <p>
                    <%=err.message%>
                </p>
                <hr>
                <p class="mb-0">
                    <%= err.stack %>
                </p>
                <a href="/campgrounds" class="btn btn-outline-secondary mt-3">Back</a>
            </div>
        </div>
    </div>