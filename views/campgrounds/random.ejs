<div class="pagination-div">
    <ul class="pagination justify-content-center">
        <li class="page-item previous">
            <a class="page-link"
                href="/campgrounds?page=<%= parseInt(page)-1 %>&search=<%= search %>">Previous</a>
        </li>
        <li class="page-item"><a class="page-link"
                href="/campgrounds?page=<%= page %>&search=<%= search %>">
                <%= page %>
            </a>
        </li>
        <li class="page-item next">
            <a class="page-link"
                href="/campgrounds?page=<%= parseInt(page)+1 %>&search=<%= search %>">Next</a>
        </li>
    </ul>
</div>

